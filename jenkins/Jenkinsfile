pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('test-Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('test-Test') {
            steps {
                sh 'mvn test'
            }
            post {
                always {
                    junit 'target/surefire-reports/*.xml'
                }
            }
        }
        
            }
        }


